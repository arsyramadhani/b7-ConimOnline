    @*<script src="~/Scripts/vendor/select2/js/select2.full.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/js/select2.full.min.js"></script>

    <!-- Volt JS -->
    <script src="~/Assets/js/volt.js"></script>

    <script type="text/javascript">$.fn.select2.defaults.set("theme", "bootstrap-5");</script>


    <script type="text/javascript">
        //const userAd = "febrian.ramadhani"
        $(document).ready(function () {
            
            let userInfo = sessionStorage.getItem("NIK")
            if (!userInfo) {
                getAuth()
            }

            console.log(JSON.parse(sessionStorage.getItem("userinfo")))

            $('#display_username').text(`WELCOME, ${JSON.parse(sessionStorage.getItem("userinfo")).USERNAME} - ${JSON.parse(sessionStorage.getItem("userinfo")).NIK}`)
             
            $('#submenu-laporan-conim').click()
        });

        const getAuth = (username = 'febrian.ramadhani') => {
            $.ajax({
                type: "POST",
                url: "/api/conim",
                data: JSON.stringify({
                    Option: "GET_AUTH",
                    UserName: username
                }),
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (res) {
                    let result = res[0]
                    console.log(result.USERNAME)
                    console.log('Fetch user data')
                    sessionStorage.setItem("userinfo", JSON.stringify(result));

                    Object.keys(result).forEach((val) => { 
                        sessionStorage.setItem(val, result[val] )
                    }) 

                },
                error: (err) => {
                    console.error(err)
                }
            }).catch(err => {
                        console.error(err)
            
            });
        }
    </script>

    @RenderSection("scripts", required: false)
